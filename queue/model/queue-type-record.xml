<record
	name="queue-type"
	type="type">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			name="parent-type"
			type="object-type"
			column="parent_object_type_id"/>

		<code-field/>

		<!-- details -->

		<description-field/>

		<!-- data -->

		<reference-field
			name="subject-type"
			type="object-type"
			column="subject_object_type_id"/>

		<reference-field
			name="ref-type"
			type="object-type"
			column="ref_object_type_id"/>

		<!-- settings -->

		<integer-field
			name="default-priority"
			nullable="yes"/>

	</fields>

</record>